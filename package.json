{
  "author": "Yamagishi Kazutoshi <ykzts@desire.sh>",
  "bugs": {
    "email": "ykzts@desire.sh",
    "url": "https://github.com/ykzts/ykzts.blog/issues"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "date-fns": "^2.8.1",
    "date-fns-tz": "^1.0.8",
    "gatsby": "^2.18.6",
    "gatsby-plugin-catch-links": "^2.1.19",
    "gatsby-plugin-emotion": "^4.1.16",
    "gatsby-plugin-feed": "^2.3.22",
    "gatsby-plugin-google-analytics": "^2.1.29",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-mdx": "^1.0.59",
    "gatsby-plugin-netlify": "^2.1.27",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-sitemap": "^2.2.22",
    "gatsby-plugin-typescript": "^2.1.20",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.1.35",
    "gatsby-remark-prismjs": "^3.3.25",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-sharp": "^2.3.6",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "description": "This repository contains a source files for ykzts.blog.",
  "devDependencies": {
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier/react"
    ],
    "overrides": [
      {
        "extends": [
          "plugin:@typescript-eslint/recommended",
          "prettier/@typescript-eslint"
        ],
        "files": [
          "*.ts",
          "*.tsx"
        ]
      },
      {
        "files": [
          "*.tsx"
        ],
        "rules": {
          "react/prop-types": "off"
        }
      }
    ],
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "error",
      "react-hooks/rules-of-hooks": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "homepage": "https://ykzts.blog/",
  "keywords": [
    "Yamagishi Kazutoshi",
    "ykzts"
  ],
  "license": "MIT",
  "main": "n/a",
  "name": "@ykzts/ykzts.blog",
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ykzts/ykzts.blog.git"
  },
  "resolutions": {
    "sharp": "0.22.1"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore ."
  },
  "version": "1.0.0"
}
